{
  "name": "nwjs-sveltekit",
  "version": "0.0.1",
  "main": "serve.js",
  "devDependencies": {
    "@sveltejs/adapter-static": "^2.0.3",
    "@sveltejs/kit": "^1.20.4",
    "@types/nw.js": "^0.13.19",
    "concurrently": "^8.2.2",
    "nw": "0.81.0-3-sdk",
    "nw-builder": "latest",
    "prettier": "^3.0.3",
    "prettier-plugin-svelte": "^3.0.3",
    "svelte": "^4.0.5",
    "svelte-check": "^3.4.3",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^4.4.2"
  },
  "node-remote": "http://localhost:*",
  "private": true,
  "scripts": {
    "dev": "concurrently \"vite dev\" \"nw .\"",
    "build": "node build.js",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "icons": "cd resources && npx png2icons ../static/favicon.png icon -allwe -bc"
  },
  "type": "module",
  "dependencies": {
    "node-static": "^0.7.11",
    "portfinder": "^1.0.32"
  }
}
